{
  "name": "native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn react-native start --config ../../../../native/rn-cli.config.js",
    "test": "jest",
    "flow": "flow"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.3"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-expo": "^4.0.0",
    "babel-preset-react-native": "4.0.0",
    "flow-bin": "^0.71.0",
    "jest": "^22.4.3",
    "jest-react-native": "^18.0.0",
    "metro-bundler-config-yarn-workspaces": "^1.0.3",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "..",
    "roots": [
      "<rootDir>/native",
      "<rootDir>/core",
      "<rootDir>/node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|core|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|sentry-expo))"
    ],
    "testMatch": [
      "**/?(*)!(.web).(spec|test).js?(x)"
    ]
  }
}
